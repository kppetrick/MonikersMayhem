{
  "name": "monikers-mayhem-server",
  "version": "1.0.0",
  "description": "Backend server for MonikersMayhem",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:formatName": "jest -t formatName",
    "test:calculateAge": "jest -t calculateAge",
    "test:getAgeRange": "jest -t getAgeRange",
    "test:coverage": "jest --coverage --collectCoverageFrom='**/models/players.js' --collectCoverageFrom='!**/node_modules/**' --collectCoverageFrom='!**/__tests__/**' --coverageReporters=text --coverageReporters=html"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "socket.io": "^4.6.0"
  },
  "devDependencies": {
    "jest": "^30.2.0",
    "nodemon": "^3.0.2"
  }
}
